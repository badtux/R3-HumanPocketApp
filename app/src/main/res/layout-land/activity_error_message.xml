    <RelativeLayout
        android:id="@+id/activity_confarm"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gray_900">

        <LinearLayout
            android:id="@+id/linearLayout_leave_category"
            android:orientation="vertical"
            android:weightSum="9"
            android:layout_width="fill_parent"
            android:background="@color/gray_900"
            android:layout_height="fill_parent">

            <LinearLayout
                android:id="@+id/linearLayout_01_0"
                android:orientation="horizontal"
                android:layout_weight="0.66"
                android:layout_width="fill_parent"
                android:layout_height="0dp">



            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout_01"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:layout_width="fill_parent"
                android:layout_height="0dp">

                <TextView
                android:text="WARNING"
                android:textStyle="bold|italic"
                android:textColor="@color/white"
                android:textSize="45sp"
                android:layout_margin="5dp"
                android:padding="10dp"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:id="@+id/tv_warnning" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout_01_1"
                android:orientation="horizontal"
                android:layout_weight="2"
                android:layout_width="fill_parent"
                android:layout_height="0dp">

                <TextView
                android:id="@+id/title_1"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:padding="5dp"
                android:gravity="left|center"
                android:text="TESTING"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:textSize="35sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout_02"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="fill_parent"
                android:layout_height="0dp">


                <TextView
                android:id="@+id/title_3"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:padding="5dp"
                android:text="TESTING"
                android:textStyle="bold"
                android:gravity="left|center"
                android:textColor="@color/white"
                android:textSize="35sp" />

            </LinearLayout>

            <LinearLayout
